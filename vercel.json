{
  "redirects": [
    {
      "source": "https://www.coxarchitecture.com/:path*",
      "destination": "https://coxarchitecture.com/:path*",
      "permanent": true
    },
    {
      "source": "http://www.coxarchitecture.com/:path*",
      "destination": "https://coxarchitecture.com/:path*",
      "permanent": true
    },
    {
      "source": "http://coxarchitecture.com/:path*",
      "destination": "https://coxarchitecture.com/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
